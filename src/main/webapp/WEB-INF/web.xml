<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.example.ilprojetfinale.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>affichage</servlet-name>
        <servlet-class>com.example.ilprojetfinale.affichage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>account</servlet-name>
        <servlet-class>com.example.ilprojetfinale.account</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>acceuilleServlet</servlet-name>
        <servlet-class>com.example.ilprojetfinale.acceuilleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editaccount</servlet-name>
        <servlet-class>com.example.ilprojetfinale.editaccount</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>editaccount</servlet-name>
        <url-pattern>/editaccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>account</servlet-name>
        <url-pattern>/account</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>acceuilleServlet</servlet-name>
        <url-pattern>/acceuilleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>affichage</servlet-name>
        <url-pattern>/affichage</url-pattern>
    </servlet-mapping>
</web-app>
